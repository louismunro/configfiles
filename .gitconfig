[user]
	name = Louis Munro
    # you chould configure this manually... 
    email = lmunro@inverse.ca

[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
    who = shortlog -s --
[init]
	templatedir = ~/.git_template
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[core]
	pager = more
[push]
	default = matching
